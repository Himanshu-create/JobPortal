@model IEnumerable<JobPortal.Models.ApplicantData>

@{
    ViewData["Title"] = "Index";
}

<h1>Applied Jobs</h1>

<table class="table">
    <thead>
        <tr>
            <th>Job name</th>
            <th>Job Domain</th>
            <th>Last date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.user.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobApplied.Domain)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobApplied.LastDateOfRegistration)
            </td>
            <td>
            </td>
        </tr>
}
    </tbody>
</table>
