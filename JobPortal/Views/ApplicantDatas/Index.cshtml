@model IEnumerable<JobPortal.Models.ApplicantData>

@{
    ViewData["Title"] = "Index";
}

<h1>Applied Jobs</h1>

<p>
    <a asp-action="JobList">Apply For new Job</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Job name</th>
            <th>Job Domain</th>
            <th>Last date</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JobApplied.jobName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobApplied.Domain)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobApplied.LastDateOfRegistration)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
